<?xml version="1.0" encoding="UTF-8"?>
<module>
	<name>
		Робокасса 54-ФЗ
	</name>
	
	<settings>
		<variable>test</variable>
		<name>Тестовый</name>
		<options>
			<name>Нет</name>
			<value>0</value>
		</options>
		<options>
			<name>Да</name>
			<value>1</value>
		</options>
	</settings>
	
	<settings>
		<variable>login</variable>
		<name>Идентификатор магазина (MerchantLogin)</name>
	</settings>
	<settings>
		<variable>password1</variable>
