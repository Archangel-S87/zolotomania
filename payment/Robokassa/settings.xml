		<options>
			<name>20%</name>
			<value>vat20</value>
		</options>
		<options>
			<name>10/110</name>
			<value>vat110</value>
		</options>
		<options>
			<name>18/118</name>
			<value>vat118</value>
		</options>
		<options>
			<name>20/120</name>
			<value>vat120</value>
		</options>
	</settings>
