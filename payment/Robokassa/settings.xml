			<value>ru</value>
		</options>
		<options>
			<name>Английский</name>
			<value>en</value>
		</options>
	</settings>
	<settings>
		<variable>vat</variable>
		<name>НДС</name>
		<options>
			<name>Без НДС</name>
			<value>none</value>
		</options>
		<options>
			<name>0%</name>
			<value>vat0</value>
		</options>
		<options>
			<name>10%</name>
			<value>vat10</value>
		</options>
		<options>
			<name>18%</name>
